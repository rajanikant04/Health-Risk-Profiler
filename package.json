{
  "name": "plum_asgn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:no-lint": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "npm run build:no-lint",
    "start": "next start",
    "start:production": "npm run start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "optimize": "npm run type-check && npm run build:production",
    "test:build": "npm run build && npm run start & sleep 5 && curl -f http://localhost:3000/api/health-check && pkill -f 'next start'",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "chart.js": "^4.5.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "tesseract.js": "^6.0.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/tesseract.js": "^0.0.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
